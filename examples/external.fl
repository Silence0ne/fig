external console {
    fn log_str(ptr: i32);
    fn log(num: i32);
}

fn len(str: i32): i32 {
    let len: i32 = 0;

    loop {
        log(str[len]);
        if (str[len] == 0) {
            break;
        }

        len = len + 1;
    }

    return len;
}

export fn main() {
    let string: i32 = "hello";
    let n_s: i32 = "helloWorld";

    log( len(string) );
}
