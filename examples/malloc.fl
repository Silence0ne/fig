builtin fn malloc(size: i32): i32;

external console {
    fn log(n: i32);
}

fn len(str_ptr: i32): i32 {
    let length: i32 = 0;

    loop {
        if (str_ptr[length] == 0) { break; };

        length = length + 1;
    }

    return length;
}

export fn main() {
    let string: i32 = "Hello World";

    log(len(string));

    return 0;
}
